{
  "name": "elfi",
  "version": "1.3.0",
  "description": "An elegant state container for your JavaScript applications",
  "main": "./index.js",
  "scripts": {
    "clean": "rimraf index.js react.js middleware/",
    "lint": "eslint src/ test/",
    "test": "babel-tape-runner test/*.js test/**/*.js",
    "build": "babel src --out-dir .",
    "check": "npm run lint && npm run test",
    "prepare": "npm run clean && npm run check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madx/elfi.git"
  },
  "keywords": [
    "flux",
    "redux",
    "state",
    "functional",
    "immutable"
  ],
  "author": "Fran√ßois Vaux <madx@yapok.org> (https://github.com/madx)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^3.19.0",
    "eslint-config-i-am-meticulous": "^5.0.0",
    "rimraf": "^2.6.1",
    "tape": "^4.8.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint-config-i-am-meticulous"
    ]
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1"
  }
}
